	public static void main(String[] args) {
	
	Scanner sc = new Scanner (System.in);
	
	double a, b, c, d;
	
	System.out.println("Informe os valores");
	a=sc.nextDouble();
	b=sc.nextDouble();
	c=sc.nextDouble();
	d=sc.nextDouble();
	System.out.println("DIFERENÃ‡A:"+((a*b)-(c*d)));
	
	sc.close();
	}
}